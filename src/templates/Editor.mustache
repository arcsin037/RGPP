	<!--<script src="{{& JS_PATH}}RGPP.editor.min.js" charset="utf-8"></script>-->
	<script src="{{& JS_PATH}}RGPP.editor.js" charset="utf-8"></script>

	<!--[if IE 8 ]> <script src="{{& LIB_PATH}}uuCanvas/uuCanvas.js" charset="utf-8"></script><![endif]-->
	<!--[if IE 7 ]> <script src="{{& LIB_PATH}}uuCanvas/uuCanvas.js" charset="utf-8"></script><![endif]-->

</head>

<body>
	<div id="icon-menu">
		<div class="btn-wrapper">
			<button class="btn" title="New Map">
				<img src="{{& SYSTEM_IMAGE_PATH}}new.svg">
			</button>
			<button class="btn" title="Save Map">
				<img src="{{& SYSTEM_IMAGE_PATH}}save.svg">
			</button>
			<button class="btn" title="Grid">
				<img src="{{& SYSTEM_IMAGE_PATH}}grid.svg">
			</button>
			<button class="btn" title="Schedule">
				<img src="{{& SYSTEM_IMAGE_PATH}}calendar.svg">
			</button>
			<button class="btn" title="Play">
				<img src="{{& SYSTEM_IMAGE_PATH}}play.svg">
			</button>
			<button class="btn" title="Pause">
				<img src="{{& SYSTEM_IMAGE_PATH}}pause.svg">
			</button>
			<button class="btn" title="Debug Mode">
				<img src="{{& SYSTEM_IMAGE_PATH}}debug.svg">
			</button>
		</div>
		<div class="selectbox-wrapper">
			<select id="executeMode" class="selectbox">
				<option value="TestMode" selected>Test Mode</option>
				<option value="EmulationMode">EmulationMode Mode</option>
			</select>
			<select id="layer" class="selectbox">
				<option value="Layer 1">Layer 1</option>
				<option value="Layer 2">Layer 2</option>
				<option value="Layer 3">Layer 3</option>
				<option value="Event" selected>Event</option>
			</select>
		</div>
	</div>
	<div id="emulation-window-panel">
		<div id="emulation-window-bar">
			<b>{{Title}}</b>
			<div id="emulation-window-buttons">
				<button class="FullScreen" title="Full Screen">
					<img src="{{& SYSTEM_IMAGE_PATH}}fullScreenIcon.svg">
				</button>
				<button class="PseudoFullScreen" title="Save Map">
					<img src="{{& SYSTEM_IMAGE_PATH}}pseudoFullScreenIcon.svg">
				</button>
				<button class="btn" title="Save Map">
					<img src="{{& SYSTEM_IMAGE_PATH}}windowIcon.svg">
				</button>
			</div>
		</div>
		<canvas id="emulation-window-canvas">
			
		</canvas>
	</div>
	<div id="palette-window" title="Palette">
		<div id="palette-window-menu">
			<select id="drawMode" class="selectbox">
				<option value="Pen" data-image="{{& SYSTEM_IMAGE_PATH}}pen.svg" data-description="Draw like using a pen." selected>Pen</option>
				<option value="Rectangle" data-image="{{& SYSTEM_IMAGE_PATH}}rectangle.svg" data-description="Fill rectangle.">Rectangle</option>
				<option value="Filling" data-image="{{& SYSTEM_IMAGE_PATH}}bucket.svg" data-description="Fill bounded areas.">Filling</option>
				<option value="Eraser" data-image="{{& SYSTEM_IMAGE_PATH}}eraser.svg" data-description="Erase map chip.">Eraser</option>
			</select>

			<label>Tag value</label>
			<input id="tag-data-spinner">

			<button id="save-tag-data" class="btn" title="Save Tag Data">
				<img src="{{& SYSTEM_IMAGE_PATH}}save.svg">
			</button>
		</div>
		<div id='PalettePanelItemDiv'>

		</div>
	</div>
	<div id="main-panel" class="main-panel">
		<section id="toolbar-left" class="toolbar">
			<div id="open-sb-left" class="menu-button menu-left"></div>
		</section>
		<section id="leftSidebar" class="leftSidebar"></section>
	</div>
	<section id="rightSidebar" class="rightSidebar"></section>
</body>

</html>